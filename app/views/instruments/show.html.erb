<div class="container">
  <p>Instrument: <%= @name %></p>
  <p>City: <%= @city %></p>


  <% if user_signed_in? %>
    <%= link_to 'Book', new_instrument_booking_path(@instrument) %>
  <% else %>
    <%= link_to 'Log in to book this instrument', new_user_session_path %>
  <% end %>
</div>
<%= link_to "Back to Instruments", instruments_path %>
